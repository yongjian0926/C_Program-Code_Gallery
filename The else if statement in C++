#include<stdio.h>

void main()
{
	double x,y;
	printf("请输入x的值：");
	scanf("%lf",&x);
	if(x<0)
	{
		y=x;
		printf("y的数值是：%.2lf\n\n",x);
	}
	else if(x>=0 && x<10)
	{
		y=2*x-1;
		printf("y的数值是：%.2lf\n\n",y);
	}
	else if(x>=10 && x<20)
	{
		y=3*x-11;
		printf("y的数值是：%.2lf\n\n",y);
	}
	else if(x>=20 && x<30)
	{
		y=4*x+5;
		printf("y的数值是：%.2lf\n\n",y);
	}
	else if(x>=30)
	{
		y=5*x-8;
		printf("y的数值是：%.2lf\n\n",y);
	}
}
