#include<stdio.h>

void main()
{
	int num;
	printf("请输入一个值：");
	scanf("%d",&num);
	switch(num)
	{
	case 1: printf("Monday\n\n");break;
	case 2: printf("Tusday\n\n");break;
	case 3: printf("Wensday\n\n");break;
	case 4: printf("Thursday\n\n");break;
	case 5: printf("Friday\n\n");break;
	case 6: printf("Saterday\n\n");break;
	case 7: printf("Sunday\n\n");break;
	default:printf("数值错误\n\n");
	}
}
